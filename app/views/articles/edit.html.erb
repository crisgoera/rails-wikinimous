<h2><%= @article.title %></h2>

<p><%= @article.content %></p>

<h3>Submit your edit</h3>

<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>

<%= link_to 'Back to index', '/' %>
